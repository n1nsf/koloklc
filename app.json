{
  "expo": {
    "name": "Kolok Learning City",
    "slug": "kolok-learning-city",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "staticRouter": {
        "basePath": "/app"
      },
      "router": {
        "basePath": "/app"
      },
      "build": {
        "output": {
          "publicPath": "/app/"
        }
      }
    },
    "plugins": [
      "expo-router",
      "expo-barcode-scanner"
    ],
    "experiments": {
      "baseUrl": "/app",
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "assetExts": [
        "glb"
      ]
    }
  }
}
